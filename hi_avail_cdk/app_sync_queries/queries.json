# Queries
query GetAllItems {
    all(limit:10){
        items2{
            name
            items2Id
        }
    }
}

mutation PutItem($name:String!) {
    save(name:$name) {
        items2Id
    }
}

query GetOneItem($id:ID!) {
    getOne(items2Id:$id) {
        items2Id
    		name
    }
}

