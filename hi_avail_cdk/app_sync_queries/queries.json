# Queries
query GetAllItems {
    all(limit:10){
        items{
            name
            itemsId
        }
    }
}

mutation PutItem($name:String!) {
    save(name:$name) {
        itemsId
    }
}

query GetOneItem($id:ID!) {
    getOne(itemsId:$id) {
        itemsId
    		name
    }
}

